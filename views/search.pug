extends layouts/main 
include mixins/videoBlock

block contents
    .search__header
        h3 search : #{searchByTerm}!
    .search__videos
        if videos.length === 0
            p there is no videos!!
        else
            each video in videos
                + videoBlock({
                    id : video.id,
                    title : video.title,
                    views : video.views,
                    videoFile : video.videoFile
                })